{
  "name": "modern-react-web-template",
  "description": "Modern React Web App Template",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjxenjoy/modern-react-web-template.git"
  },
  "keywords": [
    "react",
    "creact-react-app",
    "template",
    "single page application"
  ],
  "author": "Jinxin Hu <hjxenjoy@gmail.com> (https://hujin.xin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hjxenjoy/modern-react-web-template/issues"
  },
  "homepage": "https://github.com/hjxenjoy/modern-react-web-template#readme",
  "github": {
    "release": true
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "http-proxy-middleware": "^0.19.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "release-it": "^12.3.3",
    "sass": "^1.21.0",
    "typescript": "^3.5.2"
  }
}
